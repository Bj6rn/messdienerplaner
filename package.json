{
  "name": "messdienerplaner",
  "version": "1.0.0",
  "description": "Programm zur Verwaltung von Messdienern und dem einfachen Erstellen von Messdienerplänen.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-linux": "electron-packager . messdienerplaner --platform linux --arch x64 --out dist/ --icon=img/logo_color1.png",
    "package-windows": "electron-packager . messdienerplaner --platform win32 --arch x64 --out dist/ --icon=img/logo_color1.ico",
    "package-mac": "electron-packager . messdienerplaner --platform darwin --arch x64 --out dist/ --icon=img/logo_color1.icns"
  },
  "author": "Björn Bousart - bjoernbousart.de",
  "license": "MIT",
  "devDependencies": {
    "electron": "^23.1.1",
    "electron-packager": "^17.1.1"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "pdf-creator-node": "^2.2.4"
  }
}
